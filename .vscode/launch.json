    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 45380
        },
        {
            "name": "Launch installer",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "${workspaceRoot}\\out\\VideoGalleryInstaller.exe",
        },
        {
            "name": "Debug installer",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}\\cmd\\installer\\main.go"
        }
    ]
}